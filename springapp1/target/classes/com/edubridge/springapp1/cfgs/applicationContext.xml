<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-4.0.xsd" > 
    
    <bean class="com.edubridge.springapp1.beans.IphoneMobile" id="iphone">
    <property name="modelNo" value="9988"/>
    <property name="color" value="Grey"/>
    </bean>
    <bean class="com.edubridge.springapp1.beans.SamsungMobile" id="samsung">
    <constructor-arg value="1234"/>
    <constructor-arg value="Black"/>
    </bean> 
    	
</beans>