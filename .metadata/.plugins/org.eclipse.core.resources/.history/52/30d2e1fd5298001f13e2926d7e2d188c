package java_wrapper_class;

public class WrapperClassDemo {
	public static void main(String[] args) {
		
		// wrapper classes
		
		//autoboxing.
		int num = 34;
		Integer n = (Integer)num;		
		System.out.println(n.doubleValue());		
		
		//auto-unboxing
		num = (int)n;		
		System.out.println(num);
		
		
		
	}
}
